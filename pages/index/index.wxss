.index {
  height: 100vh;
  background-size: cover;
  visibility: hidden;
  position: relative;
}
.index .cover {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  transition: all 1s ease;
}
.index.active {
  visibility: visible;
}
.links {
  position: absolute;
  bottom: 70px;
  width: 100%;
}
navigator {
  box-sizing: border-box;
  width: 70%;
  margin: 15px auto 0;
  background: #e22d47;
  padding: 20px;
  color: #fff;
  font-size: 14px;
  display: flex;
  justify-content: space-between;
}
navigator:nth-child(2) {
  background: #466bb0;
}
navigator:nth-child(3) {
  background: #3dddc0;
}
